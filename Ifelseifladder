package Owas;

public class Ifelseifladder {  //creating a class

	public static void main(String[] args) {
		String name="Owashim" ;  //string variable 
				if(name=="riya"){
				System.out.println("yourname is riya");  

				}
				else if(name=="Rony"){
				System.out.println("yourname is Rony");
				}
				else{
					System.out.println(name);   //print name
					}

	}

}
